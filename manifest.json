{ 
    "manifest_version": 2,
    "name": "pinkP",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon@16.png"
    },
    "icons": { 
        "16": "icon@16.png",
        "48": "icon@48.png",
        "128": "icon@128.png" 
    }
}